{
    "name": "react-testing-strategies",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@storybook/addon-storyshots": "^6.1.12",
        "@testing-library/jest-dom": "^5.11.8",
        "@testing-library/react": "^11.2.2",
        "@testing-library/user-event": "^12.6.0",
        "@types/jest": "^26.0.19",
        "@types/node": "^12.19.12",
        "@types/react": "^16.14.2",
        "@types/react-dom": "^16.9.10",
        "bootstrap": "^4.5.3",
        "http-status-codes": "^2.1.4",
        "jest-fetch-mock": "^3.0.3",
        "moment": "^2.29.1",
        "react": "^17.0.1",
        "react-bootstrap": "^1.4.3",
        "react-dom": "^17.0.1",
        "react-intl": "^5.10.14",
        "react-redux": "^7.2.2",
        "react-scripts": "4.0.1",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-logger": "^3.0.6",
        "redux-promise-middleware": "^6.1.2",
        "redux-thunk": "^2.3.0",
        "typescript": "^4.1.3",
        "web-vitals": "^0.2.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "chromatic": "chromatic --project-token CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
        "test:coverage": "npm test -- --env=jsdom --coverage --watchAll=false"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@storybook/addon-actions": "^6.1.12",
        "@storybook/addon-essentials": "^6.1.12",
        "@storybook/addon-links": "^6.1.12",
        "@storybook/node-logger": "^6.1.12",
        "@storybook/preset-create-react-app": "^3.1.5",
        "@storybook/react": "^6.1.12",
        "@types/react-redux": "^7.1.9",
        "@types/redux-logger": "^3.0.8",
        "@types/redux-mock-store": "^1.0.2",
        "chromatic": "^5.5.0",
        "prettier": "^2.2.1",
        "redux-mock-store": "^1.5.4",
        "storybook-addon-intl": "^2.4.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^5.0.0"
    },
    "jest": {
        "coverageReporters": [
            "text",
            "cobertura",
            "lcovonly"
        ],
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!<rootDir>/node_modules/",
            "!src/reportWebVitals.ts",
            "!src/**/*.{test.ts,test.tsx,stories.tsx}",
            "!src/**/*-object.ts",
            "!src/@types/*.ts",
            "!src/testdata/*.ts",
            "!src/*.d.ts",
            "!src/infrastructure_tests/*"
        ]
    },
    "jest-junit": {
        "suiteName": "workdays-editor-ui",
        "outputDirectory": "coverage/junit",
        "outputName": "junit.xml",
        "usePathForSuiteName": "true"
    }
}
